<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="healthcenter">
	
	<!-- 게시글 목록 조회 -->
	<select id="selectCenter" parameterType="int" resultType="HealthCenter">
		SELECT LC.LID, LC.LNAME, H.HPLACE, H.HADDR, H.HPNUM FROM HEALTHCENTER H JOIN LOCATIONCODE LC ON H.LID = LC.LID
		<!-- SELECT * FROM HEALTHCENTER -->
		<!-- SELECT FROM HEALTHCENTER H JOIN LOCATIONCODE LC ON H.LID = LC.LID -->
	</select>
</mapper>
